                    <ui:Button text="MENU" display-tooltip-when-elided="true" name="BackToMenu" class="MenuButton MenuButtonHover" />
                    <ui:Button text="NEXT&#10;LEVEL" display-tooltip-when-elided="true" name="NextLevel" class="MenuButton MenuButtonHover" />
                    <ui:Button text="REPLAY" display-tooltip-when-elided="true" name="ResetGame" class="MenuButton MenuButtonHover" />
                </ui:GroupBox>
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="GameUI" style="width: 100%; height: 100%; align-items: stretch; display: none; visibility: hidden; flex-direction: row;">
            <ui:VisualElement name="LeftSideElement" style="position: relative; height: 100%; width: 20%; left: 0;" />
