                    <ui:Label text="Rotations: 0" display-tooltip-when-elided="true" name="RotationsCount" style="padding-left: 0; padding-right: 5%; padding-top: 3%; padding-bottom: 0; font-size: 45px; align-items: flex-start; -unity-text-align: upper-right; -unity-font-style: bold; width: 100%; height: 100%;" />
                </ui:VisualElement>
                <ui:VisualElement name="ProgressBarPanel" style="height: 438px; width: 100%; margin-left: 0; margin-top: 0; flex-direction: row; margin-right: 0; margin-bottom: 0; justify-content: flex-end;">
                    <ui:VisualElement name="StarsHolder">
                        <ui:VisualElement name="ProgressBarStar1" class="ProgressBarStarImageHolder">
                            <ui:IMGUIContainer name="StarImage1" class="ProgressBarStarImage" />
                            <ui:Label text="&lt;10" display-tooltip-when-elided="true" name="RotationsAmountToLoseStar1" class="RotationsAmountToLoseStar1" />
