                            <ui:Label text="&lt;25" display-tooltip-when-elided="true" name="RotationsAmountToLoseStar2" class="RotationsAmountToLoseStar1" />
                        </ui:VisualElement>
                        <ui:VisualElement name="ProgressBarStar3" class="ProgressBarStarImageHolder">
                            <ui:IMGUIContainer name="StarImage3" class="ProgressBarStarImage" />
                            <ui:Label text="WIN" display-tooltip-when-elided="true" name="RotationsAmountToLoseStar3" class="RotationsAmountToLoseStar1" style="margin-right: 0;" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:VisualElement name="ProgressBar" class="ProgressBarBase">
                        <ui:VisualElement name="ProgressDisplay" style="overflow: visible; background-color: rgb(63, 63, 63); width: 100%; height: 107%; padding-bottom: 2px; border-bottom-width: 2px; border-bottom-color: rgb(83, 0, 245); max-height: 95%;" />
